{
  "name": "downstairstech/downstairs",
  "type": "project",
  "version": "1.29.0",
  "description": "The Metronic in the Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "akaunting/laravel-money": "^4.0.1",
    "archtechx/enums": "^0.3.2",
    "doctrine/dbal": "^3.6",
    "dyrynda/laravel-cascade-soft-deletes": "^4.3.0",
    "genl/matice": "^1.1.7",
    "guzzlehttp/guzzle": "^7.2",
    "inertiajs/inertia-laravel": "^0.6.9",
    "jackiedo/log-reader": "^2.3",
    "kolossal-io/laravel-multiplex": "^1.0",
    "lab404/laravel-impersonate": "^1.7",
    "laravel/framework": "^10.11",
    "laravel/sanctum": "^3.2",
    "laravel/tinker": "^2.8",
    "rappasoft/laravel-authentication-log": "^3.0",
    "spatie/cpu-load-health-check": "^1.0",
    "spatie/laravel-activitylog": "^4.7",
    "spatie/laravel-ciphersweet": "^1.0",
    "spatie/laravel-data": "^3.5",
    "spatie/laravel-health": "^1.23",
    "spatie/laravel-permission": "^5.8",
    "torann/geoip": "^3.0",
    "whichbrowser/parser": "^2.1"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.8",
    "barryvdh/laravel-ide-helper": "^2.13",
    "fakerphp/faker": "^1.21",
    "laravel/breeze": "^1.18",
    "laravel/pint": "^1.2",
    "laravel/sail": "^1.18",
    "mockery/mockery": "^1.5",
    "nunomaduro/collision": "^7.0",
    "orchestra/testbench": "^8.5.3",
    "overtrue/phplint": "^9.0",
    "phpunit/phpcov": "^9.0",
    "phpunit/phpunit": "^10.1",
    "plannr/laravel-fast-refresh-database": "^1.0",
    "roave/security-advisories": "dev-latest",
    "spatie/laravel-ignition": "^2.0",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "files": [
      "app/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta"
    ]
  }
}
